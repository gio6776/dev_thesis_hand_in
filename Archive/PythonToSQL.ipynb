    "import pandas as pd\n",
    "\n",
    "# Create a simple dataframe\n",
    "df = pd.DataFrame({'A': [1, 2, 3, 4, 5]})\n",
    "\n",
    "print(\"Original DataFrame:\")\n",
    "print(df)\n",
    "\n",
    "# Use shift method\n",
    "df['B'] = df['A'].shift(1)\n",
    "\n",
    "print(\"\\nDataFrame after shift operation:\")\n",
    "print(df)"
